trans <- function(x, y)
{
data = list()
for(i in 1:nrow(x))
{
data1 = seq(x[i,1], x[i,2], (x[i,2] - x[i,1])/100)
data2 = seq(y[i,1], y[i,2], (y[i,2] - y[i,1])/100)
data[[i]] = list(data1,data2)
}
return(data)
}
cor <- function(wc)
{
w_c = unlist(wc[2])
return(w_c[2]/sqrt(w_c[1]*w_c[4]))
}
cor_xxx_zz = cor(WassCov(y = trans(mydataxxx,mydatazz)))
